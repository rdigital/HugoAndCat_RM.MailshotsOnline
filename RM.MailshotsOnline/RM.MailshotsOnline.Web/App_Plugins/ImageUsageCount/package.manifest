{   
    //you can define multiple editors   
    propertyEditors: [      
        {
            /*this must be a unique alias*/ 
            alias: "RM.HC.ImageUsageCount",
            /*the name*/
            name: "Image library usage count",
            /*the html file we will load for the editor*/
            editor: {
                view: "~/App_Plugins/ImageUsageCount/usagecount.html"
            }
        }
    ]
    ,
    //array of files we want to inject into the application on app_start
    javascript: [
        '~/App_Plugins/ImageUsageCount/usagecount.controller.js'
    ]
}