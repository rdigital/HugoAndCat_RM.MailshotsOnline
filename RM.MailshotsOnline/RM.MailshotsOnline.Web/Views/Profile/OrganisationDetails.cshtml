@inherits Glass.Mapper.Umb.Web.Ui.GlassViewPage < RM.MailshotsOnline.Entities.PageModels.Profile.OrganisationDetails>

@{
    Layout = "~/Views/Master.cshtml";
    if (Model == null)
    {
        throw new HttpException(404, "Page not found");
    }
}

<div class="content">
    <h1>@Model.Name</h1>
    @Umbraco.RenderMacro("Breadcrumbs")
    <div>
        @Html.Action("ShowEditOrganisationDetails", "ProfileSurface", new { model = Model })
    </div>
</div>
