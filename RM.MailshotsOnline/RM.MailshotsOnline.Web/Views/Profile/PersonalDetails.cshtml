@inherits Umbraco.Web.Mvc.UmbracoViewPage<RM.MailshotsOnline.Entities.PageModels.Profile.PersonalDetails>

@{
    Layout = "~/Views/Master.cshtml";
}

<div class="content">
    <h1>@Model.Name</h1>
    @Umbraco.RenderMacro("Breadcrumbs")
    <div>
        @Html.Action("ShowEditPersonalDetails", "ProfileSurface", new {model = Model})
    </div>
    <div>
        @Html.Action("ShowChangePassword", "ProfileSurface", new { model = Model })
    </div>
</div>
